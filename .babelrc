{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "transform-assets",
      {
        "extensions": [
          "txt",
          "svg",
          "png",
          "ico"
        ],
        "regExp": ".*/static/(.+)",
        "name": "/static/[1]?[sha512:hash:base64:7]"
      }
    ],
    [
      "module-resolver",
      {
        "root": [
          "."
        ],
        "alias": {
          "styles": "./styles"
        },
        "cwd": "babelrc"
      }
    ],
    [
      "wrap-in-js",
      {
        "extensions": [
          "css$",
          "less$"
        ]
      }
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "style": false
      }
    ]
  ],
  "ignore": [],
  "sourceMaps": true
}