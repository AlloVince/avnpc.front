{
  "name": "avnpc.front",
  "version": "0.0.0-development",
  "description": "Frontend of avnpc.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlloVince/avnpc.front.git"
  },
  "scripts": {
    "dev": "./node_modules/.bin/babel-node server.js",
    "dev-css": "webpack --watch --mode development --config webpack.dll.js",
    "pre-build": "atool-build --output-path=antd-custom --config=antd-custom/webpack.config.js",
    "build": "NODE_ENV=production npm run build:dll && next build",
    "build:dll": "NODE_ENV=production webpack --config webpack.dll.js -p",
    "start": "NODE_ENV=production node server.js",
    "test": "npm run coverage",
    "lint": "eslint pages/* --ext .js",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "engines": {
    "node": ">=10"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "antd": "^3.19.8",
    "autoprefixer": "^9.6.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "evaengine": "^0.11.1",
    "gitalk": "^1.5.0",
    "glob": "^7.1.4",
    "highlight.js": "^9.15.8",
    "htmlescape": "^1.1.1",
    "isomorphic-unfetch": "^3.0.0",
    "katex": "^0.10.2",
    "luxon": "^1.16.0",
    "markdown-it": "^8.4.2",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-toc-and-anchor": "^4.2.0",
    "mermaid": "^8.0.0-rc.6",
    "next": "^8.1.0",
    "next-routes": "^1.4.2",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.14.1",
    "styled-jsx": "^3.2.1",
    "url-parse": "^1.4.7",
    "xmlbuilder": "^13.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@zeit/next-source-maps": "0.0.3",
    "babel-eslint": "^10.0.2",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lost": "^8.3.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-url": "^8.0.0",
    "pre-commit": "^1.2.2",
    "raw-loader": "^3.0.0",
    "semantic-release": "^15.13.12",
    "styled-jsx-plugin-postcss": "^2.0.1",
    "travis-deploy-once": "^5.0.11",
    "url-loader": "^2.0.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "release": {
    "npmPublish": false
  },
  "postcss": {
    "plugins": {
      "lost": {},
      "postcss-nested": {}
    }
  }
}
