{
  "name": "avnpc.front",
  "version": "0.0.0-development",
  "description": "Frontend of avnpc.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlloVince/avnpc.front.git"
  },
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "ava": "LOG_LEVEL=error NODE_ENV=test node ./node_modules/.bin/ava --verbose --serial",
    "test": "npm run coverage",
    "crontab": "NODE_ENV=production babel-node ./src/crontab",
    "coverage": "nyc -a --reporter=lcov --reporter=text --reporter=html npm run ava",
    "swagger": "rm -r ./build/* && npm run build && SWAGGER_VALID=1 node build/swagger.js",
    "swagger-dev": "rm -r ./build/* && npm run build && nodemon --ignore 'node_modules/*' --ignore '**/*.json' --exec babel-node src/swagger.js",
    "lint": "eslint src/* --ext .js",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/bootstrap.js",
      "!config/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": true,
    "tap": false,
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "require": [
      "babel-core/register"
    ],
    "include": [
      "src/**/*.js"
    ]
  },
  "engines": {
    "node": ">=8.9.1"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@zeit/next-css": "0.0.7",
    "ant-design-pro": "^1.1.0",
    "antd": "^3.2.1",
    "autoprefixer": "^8.0.0",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "css-loader": "^0.28.9",
    "dotenv": "^5.0.0",
    "evaengine": "^0.7.0",
    "glob": "^7.1.2",
    "hoist-non-react-statics": "^2.3.1",
    "isomorphic-unfetch": "^2.0.0",
    "next": "^5.0.0",
    "next-routes": "^1.3.0",
    "postcss-css-variables": "^0.8.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.0",
    "pug": "^2.0.0-beta3",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "serve-favicon": "^2.4.5",
    "serve-static": "^1.13.2",
    "style-loader": "^0.20.1",
    "styled-jsx-plugin-postcss": "^0.1.2",
    "webpack": "^3.11.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "nock": "^9.1.6",
    "nyc": "^11.4.1",
    "pre-commit": "^1.2.2",
    "semantic-release": "^12.4.1",
    "travis-deploy-once": "^4.3.4"
  }
}
