{
  "name": "avnpc.front",
  "version": "0.0.0-development",
  "description": "Frontend of avnpc.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlloVince/avnpc.front.git"
  },
  "scripts": {
    "dev": "node server.js",
    "pre-build": "atool-build --output-path=antd-custom --config=antd-custom/webpack.config.js",
    "build": "NODE_ENV=production npm run build:dll && next build",
    "build:dll": "webpack --config webpack.dll.js -p",
    "start": "NODE_ENV=production node server.js",
    "test": "npm run coverage",
    "crontab": "NODE_ENV=production babel-node ./src/crontab",
    "lint": "eslint pages/* --ext .js",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "engines": {
    "node": ">=10"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "antd": "^3.9.2",
    "autoprefixer": "^9.1.5",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "evaengine": "^0.11.0",
    "gitalk": "^1.4.0",
    "glob": "^7.1.3",
    "highlight.js": "^9.12.0",
    "htmlescape": "^1.1.1",
    "isomorphic-unfetch": "^3.0.0",
    "katex": "^0.9.0",
    "luxon": "^1.3.3",
    "markdown-it": "^8.4.2",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "mermaid": "^8.0.0-rc.6",
    "next": "^7.0.0-canary.20",
    "next-routes": "^1.4.2",
    "nprogress": "^0.2.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.13.2",
    "styled-jsx": "^3.0.2",
    "url-parse": "^1.4.3",
    "xmlbuilder": "^10.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lost": "^8.3.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.0",
    "postcss-url": "^8.0.0",
    "pre-commit": "^1.2.2",
    "raw-loader": "^0.5.1",
    "semantic-release": "^15.9.15",
    "styled-jsx-plugin-postcss": "^0.1.3",
    "travis-deploy-once": "^5.0.7",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0"
  },
  "postcss": {
    "plugins": {
      "lost": {},
      "postcss-nested": {}
    }
  }
}
