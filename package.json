{
  "name": "avnpc.front",
  "version": "0.0.0-development",
  "description": "Frontend of avnpc.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlloVince/avnpc.front.git"
  },
  "scripts": {
    "dev": "node server.js",
    "pre-build": "atool-build --output-path=antd-custom --config=antd-custom/webpack.config.js",
    "build": "NODE_ENV=production npm run build:dll && next build",
    "build:dll": "webpack --config webpack.dll.js",
    "start": "NODE_ENV=production node server.js",
    "ava": "LOG_LEVEL=error NODE_ENV=test node ./node_modules/.bin/ava --verbose --serial",
    "test": "npm run coverage",
    "crontab": "NODE_ENV=production babel-node ./src/crontab",
    "coverage": "nyc -a --reporter=lcov --reporter=text --reporter=html npm run ava",
    "swagger": "rm -r ./build/* && npm run build && SWAGGER_VALID=1 node build/swagger.js",
    "swagger-dev": "rm -r ./build/* && npm run build && nodemon --ignore 'node_modules/*' --ignore '**/*.json' --exec babel-node src/swagger.js",
    "lint": "eslint src/* --ext .js",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/bootstrap.js",
      "!config/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": true,
    "tap": false,
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "require": [
      "babel-core/register"
    ],
    "include": [
      "src/**/*.js"
    ]
  },
  "engines": {
    "node": ">=8.9.1"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "antd": "^3.5.2",
    "autoprefixer": "^8.5.0",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "evaengine": "^0.9.4",
    "gitalk": "^1.2.2",
    "glob": "^7.1.2",
    "highlight.js": "^9.12.0",
    "htmlescape": "^1.1.1",
    "isomorphic-unfetch": "^2.0.0",
    "katex": "^0.9.0",
    "luxon": "^1.2.0",
    "markdown-it": "^8.4.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "mermaid": "^8.0.0-rc.6",
    "next": "^6.0.3",
    "next-routes": "^1.4.1",
    "nprogress": "^0.2.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.13.2",
    "styled-jsx": "^2.2.6",
    "url-parse": "^1.4.0",
    "xmlbuilder": "^10.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-assets": "^1.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lost": "^8.2.1",
    "nock": "^9.2.6",
    "nyc": "^11.8.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.5",
    "postcss-nested": "^3.0.0",
    "postcss-url": "^7.3.2",
    "pre-commit": "^1.2.2",
    "raw-loader": "^0.5.1",
    "semantic-release": "^15.4.1",
    "styled-jsx-plugin-postcss": "^0.1.3",
    "travis-deploy-once": "^5.0.0",
    "url-loader": "^1.0.1",
    "webpack": "^3.11.0"
  },
  "postcss": {
    "plugins": {
      "lost": {},
      "postcss-nested": {}
    }
  }
}
